syntax = "v1"

type ImageUploadRequest {}

type ImageUploadResponse {
	Url string `json:"url"`
}

type ImageShowRequest {
	ImageType string `path:"imageType"`
	ImageName string `path:"imageName"`
}

service file {
	@handler ImageUpload
	post /api/file/image_upload (ImageUploadRequest) returns (ImageUploadResponse)

	@handler ImageShow
	get /api/file/image_show/:imageType/:imageName (ImageShowRequest)
}

